{
  "expo": {
    "name": "Sentinel",
    "slug": "sentinel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sentinel",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Sentinel needs camera access to capture medical images for emergency triage analysis.",
        "NSPhotoLibraryUsageDescription": "Sentinel needs photo library access to analyze medical images for emergency triage.",
        "NSMicrophoneUsageDescription": "Sentinel needs microphone access for voice input during emergencies.",
        "NSSpeechRecognitionUsageDescription": "Allow $(PRODUCT_NAME) to use speech recognition."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mohammadkh.sentinel",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-fast-tflite",
        {
          "enableAndroidGpuLibraries": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Guardian BUET needs camera access to capture medical images for emergency triage analysis."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Guardian BUET needs photo library access to analyze medical images for emergency triage."
        }
      ],
      "expo-speech-recognition",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b70a5620-a56d-4d1a-b324-1ff8be614b75"
      }
    },
    "owner": "mohammadkh"
  }
}
